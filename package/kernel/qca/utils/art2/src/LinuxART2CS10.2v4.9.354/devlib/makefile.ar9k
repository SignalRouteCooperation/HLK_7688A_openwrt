CFLAGS += -Wno-pointer-arith -Wno-extra -Wno-implicit


CFLAGS += -DAR9300DLL -DNART_SCORPION_SUPPORT -DAH_SUPPORT_SCORPION -DAH_SUPPORT_HONEYBEE -DAH_SUPPORT_DRAGONFLY
#CFLAGS += -DAR9300DLL 
CFLAGS += -fPIC

